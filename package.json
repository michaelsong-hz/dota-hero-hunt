{
  "name": "dota-hero-hunt",
  "version": "0.6.4",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.7",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@react-hook/media-query": "^1.1.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@sentry/react": "^7.3.1",
    "@sentry/tracing": "^7.3.1",
    "bootstrap": "^5.1.3",
    "howler": "^2.2.3",
    "jquery": "^3.6.0",
    "localforage": "^1.10.0",
    "peerjs": "^1.4.6",
    "popper.js": "^1.16.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.4.5",
    "react-switch": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint:fix": "eslint --fix \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint-style": "prettier --check \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)\"",
    "lint-style:fix": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)\"",
    "get-version": "echo ${npm_package_version}",
    "deploy": "./deploy.sh",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/howler": "^2.2.7",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-bootstrap-range-slider": "^1.2.1",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "eslint --fix"
    ],
    "*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)": [
      "prettier --write"
    ]
  }
}
