{
  "name": "dota-hero-hunt",
  "version": "0.6.3",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@react-hook/media-query": "^1.1.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@sentry/react": "^6.14.0",
    "@sentry/tracing": "^6.14.0",
    "bootstrap": "^4.6.1",
    "howler": "^2.2.3",
    "jquery": "^3.6.0",
    "localforage": "^1.10.0",
    "peerjs": "^1.3.2",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-bootstrap-range-slider": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-spring": "^9.3.0",
    "react-switch": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint:fix": "eslint --fix \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint-style": "prettier --check \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)\"",
    "lint-style:fix": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)\"",
    "get-version": "echo ${npm_package_version}",
    "deploy": "./deploy.sh",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/howler": "^2.2.4",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.14.9",
    "@types/react": "^17.0.34",
    "@types/react-bootstrap-range-slider": "^1.2.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react-scripts": "^4.0.3",
    "sass": "^1.43.4",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "eslint --fix"
    ],
    "*.+(ts|tsx|js|jsx|json|yml|yaml|scss|css|md)": [
      "prettier --write"
    ]
  }
}
